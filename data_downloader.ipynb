   "source": [
    "import yfinance as yf\n",
    "import pandas as pd\n",
    "\n",
    "def download_data(ticker='AAPL', start='2018-01-01', end='2024-12-31'):\n",
    "    df = yf.download(ticker, start=start, end=end)\n",
    "    df.to_csv(f\"{ticker}.csv\")\n",
    "    print(f\"Data for {ticker} saved as {ticker}.csv\")\n",
    "    return df\n",
    "\n",
    "download_data()\n"
   ]
